---
data_dir: {{ vector_data_dir }}

sources:
  dummy_logs:
    type: demo_logs
    format: syslog
    interval: 1


sinks:
  clickhouse_logs:
    type: clickhouse
    inputs:
      - dummy_logs
    database: logs
    endpoint: http://158.160.106.78:8123
    table: logs_table
    acknowledgements:
      enabled: false
    healthcheck:
      enabled: false
    compression: gzip
    skip_unknown_fields: true
